{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
		"indent" : [
			true,
			"tabs"
		],
		"no-input-rename" : [
			false
		],
		"member-ordering" : [
			false
		]
    }
}
